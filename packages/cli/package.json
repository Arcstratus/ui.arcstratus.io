{
	"name": "@arcstratus/ui",
	"version": "0.0.4",
	"description": "CLI for adding Arcstratus UI components to your project",
	"type": "module",
	"bin": {
		"arcstratus": "./dist/index.js"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup src/index.ts --format esm --clean --minify",
		"dev": "tsup src/index.ts --format esm --watch",
		"typecheck": "tsc --noEmit"
	},
	"keywords": [
		"svelte",
		"ui",
		"components",
		"cli"
	],
	"dependencies": {
		"commander": "^12.1.0",
		"picocolors": "^1.1.1",
		"prompts": "^2.4.2"
	},
	"devDependencies": {
		"@types/node": "^22.10.2",
		"@types/prompts": "^2.4.9",
		"tsup": "^8.3.5",
		"typescript": "^5.7.2"
	}
}
